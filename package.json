{
  "name": "anypay-sdk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.15",
    "bsv": "^2.0.10",
    "jest-puppeteer": "^6.0.0",
    "react": "^17.0.2",
    "react-accessible-accordion": "^4.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.1",
    "styled-media-query": "^2.1.2",
    "txforge": "^0.2.5",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_PATH=./src/ react-scripts test",
    "test-puppeteer": "concurrently \"yarn:serve-*\" \"jest -c jest-puppeteer.config.js\" --kill-others",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "serve-build": "serve ./build -p 5671",
    "serve-example": "serve ./example -p 5672",
    "serve": "concurrently \"yarn:serve-*\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/node-logger": "^6.3.11",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.11",
    "@types/react-helmet": "^6.1.3",
    "@types/react-test-renderer": "^17.0.1",
    "babel-plugin-styled-components": "^1.13.2",
    "concurrently": "^6.3.0",
    "jest-styled-components": "^7.0.5",
    "puppeteer": "^10.4.0",
    "react-test-renderer": "^17.0.2",
    "serve": "^12.0.1"
  },
  "resolutions": {
    "styled-components": "^5",
    "babel-loader": "8.1.0"
  }
}
